{
	"name": "engram",
	"version": "0.0.0",
	"private": true,
	"workspaces": [
		"apps/*",
		"packages/*",
		"tooling/*"
	],
	"overrides": {
		"@ai-sdk/xai": {
			"zod": "$zod"
		},
		"ai": {
			"zod": "$zod"
		}
	},
	"scripts": {
		"build": "turbo run build",
		"typecheck": "turbo run typecheck",
		"dev": "turbo run dev",
		"lint": "biome check --write .",
		"format": "biome format --write .",
		"test": "vitest run",
		"infra:up": "docker-compose -f docker-compose.dev.yml up -d",
		"infra:down": "docker-compose -f docker-compose.dev.yml down",
		"prepare": "husky"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.8",
		"@engram/events": "*",
		"@engram/storage": "*",
		"@modelcontextprotocol/sdk": "^1.24.3",
		"@qdrant/js-client-rest": "^1.16.2",
		"@vitest/coverage-v8": "^3.2.4",
		"falkordb": "^6.3.1",
		"husky": "^9.1.7",
		"jsdom": "^27.3.0",
		"patch-package": "^8.0.1",
		"pino-pretty": "^13.1.3",
		"tsx": "^4.21.0",
		"turbo": "^2.6.3",
		"typescript": "^5.9.3",
		"vitest": "^3.2.3",
		"zod": "^4.1.13"
	},
	"packageManager": "npm@11.4.1",
	"dependencies": {
		"@confluentinc/kafka-javascript": "^1.7.0"
	}
}
